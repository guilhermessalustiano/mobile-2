<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MultiActivity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.279" />

    <TextView
        android:id="@+id/ola_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Olá,Usuário"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="298dp" />

    <Button
        android:id="@+id/abre_fourth_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fourth Activity"
        tools:layout_editor_absoluteX="134dp"
        tools:layout_editor_absoluteY="530dp" />

    <Button
        android:id="@+id/abre_second_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Second Activity"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="354dp" />

    <Button
        android:id="@+id/abre_third_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Third Activity"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="445dp" />

</androidx.constraintlayout.widget.ConstraintLayout>